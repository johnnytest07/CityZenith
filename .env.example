# Ibex Enterprise API key (server-side only — never expose to client)
IBEX_API_KEY=

# Ibex Enterprise base URL
IBEX_BASE_URL=https://api.ibexenterprise.com

# MapTiler API key (for OSM-based 3D vector tiles)
NEXT_PUBLIC_MAPTILER_KEY=your_maptiler_key_here

# Google Gemini API key (server-side only — never expose to client)
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini model name — update to match the model your API key has access to
# Reasoning-capable models: gemini-2.5-pro, gemini-3.0-pro, etc.
GEMINI_MODEL=gemini-2-pro
